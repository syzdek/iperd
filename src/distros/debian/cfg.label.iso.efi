   menuentry "Debian Linux @VERSION@ "@CODENAME@" Install (@ARCH@)" {
      echo   "Loading /boot/debian/@VERSION@/@ARCH@/vmlinuz ..."
      linux  /boot/debian/@VERSION@/@ARCH@/vmlinuz append -- quiet
      echo   "Loading /boot/debian/@VERSION@/@ARCH@/initrd ..."
      initrd /boot/debian/@VERSION@/@ARCH@/initrd
   }
